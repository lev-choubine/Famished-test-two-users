<style>
@import url('https://fonts.googleapis.com/css2?family=Rubik+Mono+One&display=swap');
      @import url('https://fonts.googleapis.com/css2?family=Lalezar&family=Rubik+Mono+One&display=swap');
img {
    display: inline;
    margin: auto;
}

ul {
    list-style-type: none;
    display: inline;
}

#vendorfood {
    text-align: center;
    width: 30%;
    background-color: rgba(245, 222, 179, 0.548);
    margin: auto;
    padding-top: 25px;
    padding-bottom: 25px;
    border-radius: 15px;
    margin-bottom: 10px;
}

h3 {
       text-align:center;
       font-size: 90px;
       color:antiquewhite;
       background-color: rgba(222, 184, 135, 0.705);
       font-family: 'Rubik Mono One', sans-serif;
     }

</style>

<h3> we found</h3>
<% found.forEach((p)=>{ %>
    <div id="vendorfood">
    <img src="<%= p.type_image %>" width="150px"/> 
<ul>
    <li>Vendor Name: <%= p.seller_name %></li>
    <li>Food: <%= p.type%></li>
    <li>Discription <%= p.type_info %></li>
    <li>Price: <%= p.price %></li>
</ul>
    </div>
<% })  %> 

